<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GUIMvvM.Views.DatagridProductDetailsView"
			 xmlns:vm="using:GUIMvvM.ViewModels"
			x:DataType="vm:DatagridProductDetailsViewModel"
			 >

	<StackPanel Orientation="Vertical">
		<DataGrid ItemsSource="{Binding Product}" Name="Hej" Background="Red" Height="200" AutoGenerateColumns="True">

		</DataGrid>
	</StackPanel>
</UserControl>
