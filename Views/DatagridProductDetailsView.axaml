<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GUIMvvM.Views.DatagridProductDetailsView"
			 xmlns:vm="using:GUIMvvM.ViewModels"
			x:DataType="vm:DatagridProductDetailsViewModel"
			 >

	<StackPanel Orientation="Vertical" Name="StackPanel">
		

		<Label>Navn : </Label>
		<TextBox Text="{Binding Produkt.Navn}" ></TextBox>
		<Label>SalgsPris : </Label>
		<TextBox Text="{Binding Produkt.SalgsPris}" ></TextBox>
		<Label>IndkøbsPris : </Label>
		<TextBox Text="{Binding Produkt.IndKøbsPris}" ></TextBox>
		<Label>Beskrivelse : </Label>
		<TextBox Text="{Binding Produkt.Beskrivelse}" ></TextBox>
		<Label>LagerAntal : </Label>
		<TextBox Text="{Binding Produkt.Lagerantal}" ></TextBox>
		<Label>ProduktEnhed : </Label>
		<TextBox Text="{Binding Produkt.Produktenhed}" ></TextBox>
		<Label>Hidden : </Label>
		<TextBox Text="{Binding Produkt.Hidden}" ></TextBox>
	</StackPanel>
</UserControl>
